<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="MultiBrowserTesting">
  <test name="Test1_ChromeTest">
  	<parameter name="browsername" value="chrome"/>
  		<classes>
  			<class name="TestNG.examplex_multibrowserTesting"/>
  		</classes>
  </test> 
  
  <test name="Test2_FireFoxTest">
  	<parameter name="browsername" value="firefox"/>
		<classes>
			<class name="TestNG.examplex_multibrowserTesting"/>
		</classes>
  </test>
  
  <test name="Test3_EdgeTest">
  	<parameter name="browsername" value="edge"/>
		<classes>
			<class name="TestNG.examplex_multibrowserTesting"/>
		</classes>
  </test>
</suite>
